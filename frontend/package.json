{
  "name": "frontend",
  "homepage": "./",
  "productName": "ToDOrganizer",
  "main": "./public/electron.js",
  "author": "Mariana Costa",
  "description": "ToDo app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^5.3.0",
    "cra-template": "1.0.3",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "html-to-image": "^1.3.20",
    "js-sha256": "^0.9.0",
    "jspdf": "^2.2.0",
    "react": "^16.13.1",
    "react-csv": "^2.0.3",
    "react-dom": "^16.13.1",
    "react-excel-renderer": "^1.1.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "styled-components": "^5.1.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-coverage": "react-scripts test -- --coverage",
    "eject": "react-scripts eject",
    "start-electron": "electron .",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "release": "npm run react-build && electron-builder --publish=always",
    "electron-build": "npm run react-build && electron-builder -mwl",
    "electron-build-win": "npm run react-build && electron-builder --win",
    "electron-build-mac": "npm run react-build && electron-builder --mac",
    "electron-build-linux": "npm run react-build && electron-builder --linux"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "wait-on": "^5.2.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ]
  },
  "build": {
    "appId": "todo app",
    "mac": {
      "icon": "./public/note.png",
      "category": "public.app-category.utilities"
    },
    "win": {
      "icon": "./public/note.png"
    },
    "files": [
      "build/**/*",
      "./public/electron.js",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "public"
    },
    "extraFiles": [
      {
        "from": "dist-django",
        "to": "dist-django",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
